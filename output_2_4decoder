S_0x55980f3a7bc0 .scope module, "test_decoder_2_4" "test_decoder_2_4" 2 1;
 .timescale 0 0;
v0x55980f3c8260_0 .var "ebit", 0 0;
v0x55980f3c8330_0 .var "sbits", 1 0;
v0x55980f3c8400_0 .net "ybit", 3 0, v0x55980f3c8120_0;  1 drivers
S_0x55980f3a7d40 .scope module, "obj" "decoder_2_4" 2 7, 3 2 0, S_0x55980f3a7bc0;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "s"
    .port_info 1 /INPUT 1 "en"
    .port_info 2 /OUTPUT 4 "y"
v0x55980f3a7f80_0 .net "en", 0 0, v0x55980f3c8260_0;  1 drivers
v0x55980f3c8040_0 .net "s", 1 0, v0x55980f3c8330_0;  1 drivers
v0x55980f3c8120_0 .var "y", 3 0;
E_0x55980f3a0e60 .event edge, v0x55980f3a7f80_0, v0x55980f3c8040_0;
    .scope S_0x55980f3a7d40;
T_0 ;
    %wait E_0x55980f3a0e60;
    %load/vec4 v0x55980f3a7f80_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55980f3c8120_0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55980f3c8040_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %vpi_call 3 18 "$display", "wrong choice!" {0 0 0};
    %jmp T_0.7;
T_0.2 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x55980f3c8120_0, 0, 4;
    %jmp T_0.7;
T_0.3 ;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x55980f3c8120_0, 0, 4;
    %jmp T_0.7;
T_0.4 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x55980f3c8120_0, 0, 4;
    %jmp T_0.7;
T_0.5 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x55980f3c8120_0, 0, 4;
    %jmp T_0.7;
T_0.7 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55980f3a7bc0;
T_1 ;
    %pushi/vec4 3, 3, 2;
    %store/vec4 v0x55980f3c8330_0, 0, 2;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55980f3c8260_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55980f3c8330_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55980f3c8260_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55980f3c8330_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55980f3c8260_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x55980f3c8330_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55980f3c8260_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x55980f3c8330_0, 0, 2;
