#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55d0f87845c0 .scope module, "test_encoder_4_2" "test_encoder_4_2" 2 1;
 .timescale 0 0;
v0x55d0f87a4c90_0 .var "ebit", 0 0;
v0x55d0f87a4d60_0 .var "inbit", 3 0;
v0x55d0f87a4e30_0 .net "ybit", 1 0, v0x55d0f87a4b20_0;  1 drivers
S_0x55d0f8784740 .scope module, "obj" "encoder_4_2" 2 7, 3 1 0, S_0x55d0f87845c0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "in"
    .port_info 1 /INPUT 1 "en"
    .port_info 2 /OUTPUT 2 "y"
v0x55d0f8784980_0 .net "en", 0 0, v0x55d0f87a4c90_0;  1 drivers
v0x55d0f87a4a40_0 .net "in", 3 0, v0x55d0f87a4d60_0;  1 drivers
v0x55d0f87a4b20_0 .var "y", 1 0;
E_0x55d0f8783440 .event edge, v0x55d0f8784980_0, v0x55d0f87a4a40_0;
    .scope S_0x55d0f8784740;
T_0 ;
    %wait E_0x55d0f8783440;
    %load/vec4 v0x55d0f8784980_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55d0f87a4b20_0, 0, 2;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55d0f87a4a40_0;
    %dup/vec4;
    %pushi/vec4 1, 0, 4;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 4;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 4;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 8, 0, 4;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %jmp T_0.6;
T_0.2 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55d0f87a4b20_0, 0, 2;
    %jmp T_0.6;
T_0.3 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x55d0f87a4b20_0, 0, 2;
    %jmp T_0.6;
T_0.4 ;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x55d0f87a4b20_0, 0, 2;
    %jmp T_0.6;
T_0.5 ;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x55d0f87a4b20_0, 0, 2;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55d0f87845c0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d0f87a4c90_0, 0, 1;
    %pushi/vec4 15, 15, 4;
    %store/vec4 v0x55d0f87a4d60_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d0f87a4c90_0, 0, 1;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x55d0f87a4d60_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d0f87a4c90_0, 0, 1;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x55d0f87a4d60_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d0f87a4c90_0, 0, 1;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x55d0f87a4d60_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d0f87a4c90_0, 0, 1;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x55d0f87a4d60_0, 0, 4;
    %delay 20, 0;
    %end;
    .thread T_1;
    .scope S_0x55d0f87845c0;
T_2 ;
    %vpi_call 2 25 "$dumpfile", "4_2_encoder.vcd" {0 0 0};
    %vpi_call 2 26 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55d0f87845c0 {0 0 0};
    %vpi_call 2 27 "$monitor", "time = %2d,   EN=%1b,     y3y2y1y0 =%2b,    A1 A0 =%2b", $time, v0x55d0f87a4c90_0, v0x55d0f87a4d60_0, v0x55d0f87a4e30_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
